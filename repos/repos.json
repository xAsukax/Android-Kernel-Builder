[{
  "kernelSource": {
    "name": "kona_with_kernelsu",
    "repo": "https://github.com/youngguo18/android_kernel_oppo_sm8250",
    "branch": "12.1",
    "device": "kona",
    "defconfig": "kona_defconfig"
  },
    "withKernelSU": true,
    "toolchains": [
      {
        "repo": "https://github.com/EmanuelCN/zyc_clang-14",
        "branch": "master",
        "name": "zyc-clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "zyc-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "zyc-clang/bin/aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "zyc-clang/bin/arm-linux-androideabi-",
        "LD": "zyc-clang/bin/ld.lld",
        "AR": "zyc-clang/bin/llvm-ar",
        "NM": "zyc-clang/bin/llvm-nm",
        "OBJCOPY": "zyc-clang/bin/llvm-objcopy",
        "OBJDUMP": "zyc-clang/bin/llvm-objdump",
        "READELF": "zyc-clang/bin/llvm-readelf",
        "OBJSIZE": "zyc-clang/bin/llvm-size",
        "STRIP": "zyc-clang/bin/llvm-strip",
        "LDGOLD": "zyc-clang/bin/aarch64-linux-gnu-ld.gold",
        "LLVM_AR": "zyc-clang/bin/llvm-ar",
        "LLVM_DIS": "zyc-clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/xAsukax/AnyKernel3",
      "branch": "kona"
    }
  }]

